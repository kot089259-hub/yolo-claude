[
  {
    "start": 0,
    "end": 4.72,
    "text": "3日間くらい北海道旅行したんですけど"
  },
  {
    "start": 4.72,
    "end": 8.36,
    "text": "楽しかった点3つ挙げていきたいと思います"
  },
  {
    "start": 8.36,
    "end": 9.66,
    "text": "1つ目は"
  },
  {
    "start": 9.66,
    "end": 13.14,
    "text": "札幌駅から"
  },
  {
    "start": 13.14,
    "end": 14.52,
    "text": "鈴木の駅まで"
  },
  {
    "start": 14.52,
    "end": 16.88,
    "text": "2駅分くらい歩いたんですけど"
  },
  {
    "start": 16.88,
    "end": 18.72,
    "text": "歩いている時の"
  },
  {
    "start": 18.72,
    "end": 20.08,
    "text": "雪道での"
  },
  {
    "start": 20.08,
    "end": 22.78,
    "text": "やっぱりみんなの争いが"
  },
  {
    "start": 22.78,
    "end": 24.92,
    "text": "結構楽しかったです"
  },
  {
    "start": 24.92,
    "end": 25.9,
    "text": "雪っていうのもあって"
  },
  {
    "start": 25.9,
    "end": 28.36,
    "text": "みんな投げ飛ばしたりして"
  },
  {
    "start": 28.36,
    "end": 30.52,
    "text": "遊んでいるのは"
  },
  {
    "start": 30.52,
    "end": 31.48,
    "text": "すごく楽しかったです"
  },
  {
    "start": 31.48,
    "end": 32.82,
    "text": "2点目は"
  },
  {
    "start": 32.82,
    "end": 35.6,
    "text": "みんなが夜のお店で"
  },
  {
    "start": 35.6,
    "end": 38.86,
    "text": "シャンパンとかやっている人もいたし"
  },
  {
    "start": 38.86,
    "end": 41.92,
    "text": "デキュラ観覧車をしている人もいたし"
  },
  {
    "start": 41.92,
    "end": 42.66,
    "text": "そういう"
  },
  {
    "start": 42.66,
    "end": 44.56,
    "text": "いつもはできないような"
  },
  {
    "start": 44.56,
    "end": 46.2,
    "text": "遊びをお金を使って"
  },
  {
    "start": 46.2,
    "end": 48.72,
    "text": "やるのはすごく楽しかったです"
  },
  {
    "start": 48.72,
    "end": 49.56,
    "text": "3つ目は"
  },
  {
    "start": 49.56,
    "end": 52.34,
    "text": "海鮮丼食べたんですけど"
  },
  {
    "start": 52.34,
    "end": 53.66,
    "text": "すごく美味しくて"
  },
  {
    "start": 53.66,
    "end": 55.54,
    "text": "それも印象に残っています"
  },
  {
    "start": 55.54,
    "end": 57.54,
    "text": "この3点ですね"
  },
  {
    "start": 57.54,
    "end": 60,
    "text": "北海道旅行楽しかったです"
  },
  {
    "start": 60,
    "end": 60.98,
    "text": "ありがとうございます"
  }
]